{
    "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
    },
    "variables": {
        "templateBaseUrl":"https://raw.githubusercontent.com/jtc127/ARMTemplates/master/",
        "StorageAccountTemplateUrl":"[concat(variables('templateBaseUrl'),'StandardStorageAccountTemplate.json')]",
        "StorageAccountParametersUrl":"[concat(variables('templateBaseUrl'),'StandardStorageAccountParameters.json')]"
    },
    "resources": [
        {
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2018-05-01",
            "name": "StorageAccount",
            "properties": {
            "mode": "Incremental",
            "templateLink": {
                "uri":"[variables('StorageAccountTemplateUrl')]",
                "contentVersion":"1.0.0.0"
            },
            "parametersLink":{
                "uri": "[variables('StorageAccountParametersUrl')]",
                "contentVersion": "1.0.0.0"
            }
            }
          }
      ],
    "outputs": {

    }
}
